{
	"name": "vscode-webview-toolkit",
	"version": "0.3.0",
	"description": "A component library for building webview-based extensions in Visual Studio Code.",
	"homepage": "https://github.com/microsoft/vscode-webview-toolkit#readme",
	"license": "MIT",
	"author": {
		"name": "Microsoft Corporation"
	},
	"bugs": {
		"url": "https://github.com/microsoft/vscode-webview-toolkit/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/microsoft/vscode-webview-toolkit.git"
	},
	"main": "dist/esm/index.js",
	"types": "dist/dts/index.d.ts",
	"scripts": {
		"start": "start-storybook -p 6006",
		"build": "rollup -c && tsc -p ./tsconfig.json && npm run doc",
		"build:storybook": "build-storybook",
		"doc": "api-extractor run --local",
		"fmt": "prettier --config ./.prettierrc --write \"**/*.{ts,js}\"",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts --fix",
		"prepublishOnly": "npm run build",
		"test": "jest --verbose",
		"test:coverage": "jest --verbose --coverage"
	},
	"dependencies": {
		"@microsoft/fast-element": "^1.0.1",
		"@microsoft/fast-foundation": "^1.16.1",
		"lodash-es": "^4.17.21",
		"vscode-codicons": "0.0.15"
	},
	"devDependencies": {
		"@microsoft/api-extractor": "^7.15.1",
		"@microsoft/eslint-config-fast-dna": "^1.2.0",
		"@rollup/plugin-commonjs": "^17.1.0",
		"@rollup/plugin-node-resolve": "^11.2.0",
		"@rollup/plugin-typescript": "^8.2.0",
		"@storybook/addon-a11y": "^6.2.9",
		"@storybook/addon-actions": "^6.2.9",
		"@storybook/addon-essentials": "^6.2.9",
		"@storybook/addon-links": "^6.2.9",
		"@storybook/html": "^6.2.9",
		"@storybook/theming": "^6.2.9",
		"@types/jest": "^26.0.20",
		"eslint-config-prettier": "^8.1.0",
		"jest": "^26.6.3",
		"prettier": "^2.2.1",
		"rollup": "^2.40.0",
		"rollup-plugin-delete": "^2.0.0",
		"rollup-plugin-filesize": "^9.1.1",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-transform-tagged-template": "0.0.3",
		"tslib": "^2.1.0",
		"typescript": "^4.2.3",
		"webpack": "^4.46.0"
	}
}
